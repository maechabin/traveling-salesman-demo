{
  "extends": "airbnb",
  "plugins": ["react-hooks"],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
  "globals": {
    "describe": false,
    "it": false,
    "test": false,
    "expect": false,
    "before": false,
    "jest": false,
    "window": false
  }
}
