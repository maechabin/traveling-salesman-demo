{
  "name": "traveling-salesman-demo",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/googlemaps": "^3.30.19",
    "@types/node": "^11.11.3",
    "@types/puppeteer": "^1.12.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^7.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "jest-enzyme": "^7.0.2",
    "puppeteer": "^1.14.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.4",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "e2e": "ts-node --project ./e2e/tsconfig.e2e.json ./e2e/demo.e2e-test.ts",
    "eject": "react-scripts eject",
    "deploy": "netlify deploy --dir ./build",
    "deploy:ci": "./node_modules/netlify-cli/bin/run deploy --site $NETLIFY_SITE_ID --auth $NETLIFY_AUTH_TOKEN --dir ./build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react-test-renderer": "^16.8.1",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "netlify-cli": "^2.9.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.1",
    "ts-node": "^8.0.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
